{
  "name": "udagram",
  "version": "1.0.0",
  "description": "Udagram is an instagram like app, where you can create an account and upload images to the cloud!",
  "main": "www/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
